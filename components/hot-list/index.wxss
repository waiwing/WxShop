@import "../../wxss/sleeve.wxss"
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 40rpx;
    background-color: white;

}

.title {
    width: 690rpx;
    height: 90rpx;
}

.inner-container {
    display: flex;
    flex-direction: row;
    width: 100%;
    box-sizing: content-box;
    justify-content: space-between;
}

.left {
    width: 346rpx;
    display: flex;
    height: 522rpx;
}

.right-container {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.right-size {
    width: 400rpx;
    height: 260rpx;
    display: flex;
}